{
  "name": "formast",
  "version": "3.0.1",
  "description": "基于JSON的企业级表单低代码框架",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev:antd": "parcel .examples/antd/index.html --out-dir .cache/.antd",
    "dev:gpt": "parcel .examples/antd/gpt/index.html --out-dir .cache/.antd-gpt",
    "dev:vue": "parcel .examples/vue/index.html --out-dir .cache/.vue",
    "dev:react": "parcel .examples/react/index.html --out-dir .cache/.react",
    "dev:tdesign-vue": "parcel .examples/tdesign-vue/index.html --out-dir .cache/.tdesign-vue",
    "build:dist": "webpack --config .build/webpack.config.js",
    "build:cjs": "babel src -d . --keep-file-extension && lessc src/_shared/index.less theme-react/index.css && lessc src/_shared/index.less theme-vue/index.css",
    "clear": "rimraf index.js antd core dist react theme-react vue theme-vue tdesign-vue _shared",
    "build": "npm run clear && cross-env NODE_ENV=production npm run build:cjs && cross-env NODE_ENV=production npm run build:dist",
    "lint": "eslint --ext .js,.jsx src"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tencent-cdc/formast.git"
  },
  "keywords": [
    "Form",
    "Dynamic",
    "Schema",
    "JSON",
    "DSL"
  ],
  "author": "tisontang",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "antd": "^4.16.8",
    "react": "^17.0.2",
    "scopex": "^4.3.3",
    "tdesign-vue": "^1.1.1",
    "ts-fns": "^10.9.1",
    "tyshemo": "^12.9.11",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.13",
    "@vue/component-compiler-utils": "^3.2.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "eslint": "^7.24.0",
    "eslint-config-tencent": "^1.0.2",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "json-schema": "^0.4.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.4.1",
    "node-forge": "^1.2.0",
    "parcel-bundler": "^1.12.4",
    "prettier-eslint": "^12.0.0",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "underscore": "^1.13.1",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.31.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "react": "./node_modules/react",
    "react-dom": "./node_modules/react-dom",
    "vue": "./node_modules/vue"
  }
}
