{
  "model": {
    "name": {
      "default": "",
      "type": "string",
      "required": true,
      "validators": [
        "required('{label}必填')"
      ],
      "label": "姓名",
      "placeholder": "请输入姓名"
    },
    "age": {
      "default": "",
      "type": "&int",
      "required": true,
      "validators": [
        "required('{label}必填')",
        "min(10, '{label}必须大于10')",
        "max(100, '{label}必须小于100')"
      ],
      "label": "年龄"
    },
    "sex": {
      "label": "性别",
      "default": "M",
      "options": [
        {
          "label": "男",
          "value": "M"
        },
        {
          "label": "女",
          "value": "F"
        }
      ],
      "disabled()": "age < 10",
      "required": true,
      "validators": [
        "required('{label}必填')"
      ]
    },
    "height": {
      "default": 0,
      "compute": "{ age * 5 }"
    },
    "pet": {
      "label": "宠物"
    },
    "<pet>": {
      "kind": {
        "default": "",
        "label": "种类"
      },
      "name": {
        "default": "",
        "label": "名字"
      }
    },
    "<books>": [
      {
        "title": {
          "default": "",
          "label": "书名"
        },
        "price": {
          "default": null,
          "label": "价格"
        }
      }
    ]
  },
  "declare": {
    "props": [
      "onSubmit",
      "random"
    ],
    "components": [
      "Input",
      "Select",
      "Label"
    ]
  },
  "layout": {
    "type": "form",
    "props": {
      "target": "some-iframe",
      "action": "/",
      "method": "post"
    },
    "class": "form",
    "events": {
      "submit(e)": "{ onSubmit(e) }"
    },
    "children": [
      {
        "type": "div",
        "vars": {
          "x": 12
        },
        "children": [
          {
            "type": "label",
            "children": [
              "变量"
            ]
          },
          {
            "type": "input",
            "vars": {
              "value": "{ x }"
            },
            "attrs": {
              "value": "{ value }",
              "placeholder": "无模型支持的vars使用，vars.value 是从父级作用域传入进来生成的"
            },
            "events": {
              "input(e)": "{ value = e.target.value }"
            }
          }
        ]
      },
      {
        "type": "div",
        "children": [
          {
            "type": "label",
            "children": "{ name.label }"
          },
          {
            "type": "input",
            "attrs": {
              "value": "{ name.value }",
              "placeholder": "{ name.placeholder }"
            },
            "events": {
              "input(e)": "{ name.value = e.target.value }"
            }
          }
        ]
      },
      {
        "type": "div",
        "children": [
          {
            "type": "label",
            "children": [
              "{ age.label }"
            ]
          },
          {
            "type": "input",
            "attrs": {
              "type": "number",
              "value": "{ age.value }",
              "placeholder": "{ age.placeholder }",
              "required": "{ age.required }"
            },
            "events": {
              "input(e)": "{ age.value = e.target.value }"
            }
          }
        ]
      },
      {
        "type": "div",
        "visible": "{ age.value > 10 }",
        "children": [
          {
            "type": "label",
            "children": [
              "{ sex.label }"
            ]
          },
          {
            "type": "select",
            "props": {
              "value": "{ sex.value }"
            },
            "children": [
              {
                "type": "option",
                "repeat": "item in {sex.options}",
                "attrs": {
                  "selected": "{ sex.value === item.value }",
                  "value": "{ item.value }"
                },
                "children": "{ item.label }"
              }
            ],
            "events": {
              "input(e)": "{ sex.value = e.target.value }"
            }
          }
        ]
      },
      {
        "type": "strong",
        "children": "宠物："
      },
      {
        "type": "div",
        "children": [
          {
            "type": "label",
            "children": "{ pet.value..kind.label }"
          },
          {
            "type": "input",
            "attrs": {
              "value": "{ pet.value..kind.value }"
            },
            "events": {
              "input(e)": "{ pet.value..kind.value = e.target.value }"
            }
          }
        ]
      },
      {
        "type": "div",
        "model": "pet",
        "children": [
          {
            "type": "label",
            "children": "{ name.label }"
          },
          {
            "type": "input",
            "attrs": {
              "value": "{ name.value }",
              "placeholder": "请输入宠物名字"
            },
            "events": {
              "input(e)": "{ name.value = e.target.value }"
            }
          }
        ]
      },
      {
        "type": "strong",
        "children": [
          "书籍列表：",
          {
            "type": "button",
            "attrs": {
              "type": "button"
            },
            "events": {
              "click()": "{ books.value.push({}) }"
            },
            "children": "添加"
          }
        ]
      },
      {
        "type": "div",
        "repeat": "book,index,books in books",
        "model": "{ book }",
        "children": [
          {
            "type": "div",
            "children": [
              {
                "type": "label",
                "children": "{ title.label }"
              },
              {
                "type": "input",
                "attrs": {
                  "value": "{ title.value }"
                },
                "events": {
                  "input(e)": "{ title.value = e.target.value }"
                }
              }
            ]
          },
          {
            "type": "div",
            "children": [
              {
                "type": "label",
                "children": "{ price.label }"
              },
              {
                "type": "input",
                "attrs": {
                  "type": "number",
                  "value": "{ price.value }"
                },
                "events": {
                  "input(e)": "{ price.value = e.target.value }"
                }
              }
            ]
          },
          {
            "type": "button",
            "attrs": {
              "type": "button"
            },
            "events": {
              "click()": "{ books.remove(book) }"
            },
            "children": "删除"
          }
        ]
      },
      {
        "type": "br"
      },
      {
        "type": "br"
      },
      {
        "type": "div",
        "children": [
          {
            "type": "button",
            "props": {
              "type": "submit"
            },
            "children": "查看结果"
          }
        ]
      },
      {
        "type": "div",
        "children": "{ random }"
      }
    ]
  }
}
