{
  "model": {
    "name": {
      "default": "",
      "type": "string",
      "required": true,
      "validators": [
        "required('{label}必填')"
      ],
      "label": "姓名",
      "placeholder": "请输入姓名"
    },
    "age": {
      "default": "",
      "type": "&int",
      "required": true,
      "validators": [
        "required('{label}必填')",
        "min(10, '{label}必须大于10')",
        "max(100, '{label}必须小于100')"
      ],
      "label": "年龄"
    },
    "sex": {
      "label": "性别",
      "default": "M",
      "options": [
        {
          "text": "男",
          "value": "M"
        },
        {
          "text": "女",
          "value": "F"
        }
      ],
      "disabled()": "age < 10",
      "required": true,
      "validators": [
        "required('{label}必填')"
      ]
    },
    "height": {
      "default": 0,
      "compute": "{ age * 5 }"
    },
    "pet": {
      "label": "宠物"
    },
    "<pet>": {
      "kind": {
        "default": "",
        "label": "种类"
      },
      "name": {
        "default": "",
        "label": "名字"
      }
    },
    "<books>": [
      {
        "title": {
          "default": "",
          "label": "书名"
        },
        "price": {
          "default": null,
          "label": "价格"
        }
      }
    ]
  },
  "declare": {
    "props": [
      "onSubmit",
      "random"
    ],
    "components": [
      "Input",
      "Select",
      "Label"
    ]
  },
  "layout": {
    "type": "form",
    "props": {
      "target": "some-iframe",
      "action": "/",
      "method": "post",
      "class": "form",
      "onSubmit(e)": "{ onSubmit(e) }"
    },
    "children": [
      {
        "type": "div",
        "vars": {
          "x": 12
        },
        "children": [
          {
            "type": "label",
            "children": [
              "变量"
            ]
          },
          {
            "type": "input",
            "vars": {
              "value": "{ x }"
            },
            "props": {
              "value": "{ value }",
              "onChange(e)": "{ value = e.target.value }",
              "placeholder": "无模型支持的vars使用，vars.value 是从父级作用域传入进来生成的"
            }
          }
        ]
      },
      {
        "#comment": "关注这里，没有传入 props，自动绑定到了 name 字段，必须使用一个经过 connect 之后的组件",
        "type": "div",
        "children": [
          {
            "type": "Label",
            "bind": "name"
          },
          {
            "type": "Input",
            "bind": "name"
          }
        ]
      },
      {
        "type": "FormItem",
        "props": {
          "label": "{ age.label }"
        },
        "children": [
          {
            "type": "InputNumber",
            "props": {
              "value": "{ age.value }",
              "onChange(e)": "{ age.value = e.target.value }",
              "placeholder": "{ age.placeholder }",
              "required": "{ age.required }"
            }
          }
        ]
      },
      {
        "type": "div",
        "children": [
          {
            "type": "label",
            "children": [
              "{ sex.label }"
            ]
          },
          {
            "type": "Select",
            "props": {
              "options": "{ sex.options }",
              "value": "{ sex.value }",
              "onChange(e)": "{ sex.value = e.target.value }",
              "placeholder": "{ sex.placeholder }",
              "required": "{ sex.required }",
              "disabled": "{ sex.disabled }"
            }
          }
        ]
      },
      {
        "type": "strong",
        "children": "宠物："
      },
      {
        "type": "div",
        "children": [
          {
            "type": "Label",
            "bind": "pet.kind"
          },
          {
            "type": "Input",
            "bind": "pet.kind"
          }
        ]
      },
      {
        "type": "div",
        "children": [
          {
            "type": "Label",
            "model": "{ pet.value }",
            "bind": "name"
          },
          {
            "type": "Input",
            "model": "pet",
            "bind": "name"
          }
        ]
      },
      {
        "type": "div",
        "model": "pet",
        "children": [
          {
            "type": "span",
            "model": true,
            "children": [
              "继承模型：",
              "{ name.value }"
            ]
          }
        ]
      },
      {
        "type": "strong",
        "children": [
          "书籍列表：",
          {
            "type": "button",
            "props": {
              "type": "button",
              "onClick()": "{ books.value.push({}) }"
            },
            "children": "添加"
          }
        ]
      },
      {
        "type": "div",
        "repeat": "book,index,books in books",
        "model": "{ book }",
        "children": [
          {
            "type": "div",
            "model": true,
            "children": [
              {
                "type": "Label",
                "model": true,
                "bind": "title"
              },
              {
                "type": "Input",
                "model": true,
                "bind": "title"
              }
            ]
          },
          {
            "type": "div",
            "model": true,
            "children": [
              {
                "type": "Label",
                "model": true,
                "bind": "price"
              },
              {
                "type": "Input",
                "model": true,
                "bind": "price"
              }
            ]
          },
          {
            "type": "button",
            "props": {
              "type": "button",
              "onClick()": "{ books.remove(book) }"
            },
            "children": "删除"
          }
        ]
      },
      {
        "type": "br"
      },
      {
        "type": "br"
      },
      {
        "type": "div",
        "children": [
          {
            "type": "button",
            "props": {
              "type": "submit"
            },
            "children": "查看结果"
          }
        ]
      },
      {
        "type": "div",
        "children": "{ random }"
      }
    ]
  }
}
